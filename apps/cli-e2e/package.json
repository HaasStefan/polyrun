{
  "name": "@polyrun/cli-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "@polyrun/cli"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/cli-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "@polyrun/cli:build",
          "@polyrun/cli:serve"
        ]
      }
    }
  }
}
